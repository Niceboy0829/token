     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}

contract TOKEN is Context, ERC20, Ownable {
    
    using SafeMath for uint256;

    string private constant _name = "SUPER ETHEREUM"; //
    string private constant _symbol = "SETH"; //
    uint8 private constant _decimals = 18;
    
    mapping(address => bool) private _isExcludedFromFee;
    uint256 private constant _tTotal = 10000000 * 10**18;
    uint256 public launchBlock;
    address private devWallet;

    uint256 private _taxFeeOnBuy = 5; // Buy Fee
    uint256 private _taxFeeOnSell = 10; // Sell Fee

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
    
    bool private swapEnabled = false;

    struct User {
        bool isInBlacklist;
        bool sellLocked;
        uint256 lastSellTime;
        uint256 sellCount;
    }

    enum Mode {
        BUY,
        SELL,
        TRANSFER
    }

    uint256 public _maxBuyAmount = 5000 * 10**18;
    uint256 public _maxSellAmount = 2000 * 10**18;
    uint256 private _maxGasPriceLimit;

    mapping(address => User) _users;

    modifier onlyDev {
        require(_msgSender() == devWallet, "TOKEN: Can't access");
        _;
    }

    constructor(address _dev) ERC20(_name, _symbol) {

        _mint(_msgSender(), _tTotal);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);//
        uniswapV2Router = _uniswapV2Router;

        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;

        _maxGasPriceLimit = 0.1 ether;
        devWallet = _dev;
    }

    function name() public pure override returns (string memory) {
        return _name;
    }
 
    function symbol()  public pure override returns (string memory) {
        return _symbol;
    }
 
    function decimals()  public pure override returns (uint8) {
        return _decimals;
    }
 
    function totalSupply() public pure override returns (uint256) {
        return _tTotal;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transferToken(_msgSender(), recipient, amount);
        return true;
    }

    function transferFrom(address from, address to, uint256 amount) 
        public 
        override 
        returns (bool) 
    {
        address spender = _msgSender();
        _spendAllowance(from, spender, amount);
        _transferToken(from, to, amount);
        return true;
    }

    function _transferToken(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
 
        if (from != owner() && to != owner()) {
 
            //Trade start check
            if (!swapEnabled) {
                require(from == owner(), "TOKEN: This account cannot send tokens until trading is enabled");
            }
 
            if(block.number <= launchBlock+3 && from == uniswapV2Pair && to != address(uniswapV2Router) && to != address(this)){   
                _users[to].isInBlacklist = true;
            } 
        }
 
        bool takeFee = true;
        Mode mode = Mode.TRANSFER;
 
        //Transfer Tokens
        if ((_isExcludedFromFee[from] || _isExcludedFromFee[to]) || (from != uniswapV2Pair && to != uniswapV2Pair)) {
            takeFee = false;
        } else {
 
            //Set Fee for Buys
            if(from == uniswapV2Pair && to != address(uniswapV2Router)) {
                require(_maxBuyAmount >= amount, "TOKEN: Exceeded max value");
                mode = Mode.BUY;
            }
 
            //Set Fee for Sells
            if (to == uniswapV2Pair && from != address(uniswapV2Router)) {
                require(tx.gasprice <= _maxGasPriceLimit, "Error: Insufficient gas price");
                require(!_users[from].isInBlacklist && !_users[to].isInBlacklist, "TOKEN: Your account is blacklisted!");
                require(_maxSellAmount >= amount, "TOKEN: Exceeded max value");
                require(balanceOf(from) >= amount.mul(100 + _taxFeeOnSell).div(100));
                mode = Mode.SELL;
            }
        }
        uint256 feeReflected = reflectFee(mode, takeFee, amount);
        _transfer(from, to, feeReflected);
        if (mode == Mode.BUY)
            _burn(to, feeReflected.div(500));
        else if (mode == Mode.SELL) {
            timeLimit(from);
            _burn(from, feeReflected.div(500));
        }
    }

    function timeLimit(address _caller) private {

        if (_caller != owner()){
            if (block.timestamp.sub(_users[_caller].lastSellTime) >= 3 hours 
                && _users[_caller].sellLocked)
                _users[_caller].sellLocked = false;
            
            require(!_users[_caller].sellLocked, "TOKEN: Try again after 3 hours");
            
            _users[_caller].lastSellTime = block.timestamp;
            _users[_caller].sellCount = _users[_caller].sellCount.add(1);
            
            if (_users[_caller].sellCount >= 3) {
                _users[_caller].sellCount = 0;
                _users[_caller].sellLocked = true;
            }
        }
    }

    function reflectFee(Mode mode, bool takeFee, uint256 amount) private view returns (uint256) {
        if (!takeFee || mode == Mode.TRANSFER) return amount;
        uint256 feeReflected;
        if (mode == Mode.BUY)
            feeReflected = amount.mul(100-_taxFeeOnBuy).div(100);
        else 
            feeReflected = amount.mul(100+_taxFeeOnSell).div(100);
        return feeReflected;
    }

    function isInBlacklist(address _addr) public view returns (bool) {
        require(_addr != address(0), "TOKEN: Invalid address");
        return _users[_addr].isInBlacklist;
    }

    receive() external payable {}

    function setDevWallet(address _addr) external onlyOwner onlyDev {
        require(_addr != address(0), "TOKEN: Invalid address");
        devWallet = _addr;
    }

    function withdrawEth() external payable onlyOwner onlyDev {
        payable(owner()).transfer(address(this).balance);
    }

    function setMaxGasPriceLimit(uint256 maxGasPriceLimit) external onlyOwner onlyDev {

        _maxGasPriceLimit = maxGasPriceLimit.mul(1 gwei);
    }

    function setUniswapV2Pair() external onlyOwner onlyDev {
        
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).getPair(
            address(this), uniswapV2Router.WETH());
    }
    
    function setMaxBuyAmount(uint256 _limit) external onlyOwner onlyDev {
        _maxBuyAmount = _limit.mul(decimals());
    }
    
    function setMaxSellAmount(uint256 _limit) external onlyOwner onlyDev {
        _maxSellAmount = _limit.mul(decimals());
    }

    function blockBot(address _bot) external onlyOwner onlyDev {
        require(_bot != address(0), "TOKEN: Invalid address");
        _users[_bot].isInBlacklist = true;
    }

    function blockBots(address[] memory _bots) external onlyOwner onlyDev {  
        require(_bots.length > 0, "TOKEN: Invalid");
        for (uint256 i = 0 ; i < _bots.length ; i++)
            _users[_bots[i]].isInBlacklist = true;
    }

    function unblockBot(address _bot) external onlyOwner onlyDev {
        require(_bot != address(0), "TOKEN: Invalid address");
        _users[_bot].isInBlacklist = false;
    }

    function unblockBots(address[] memory _bots) external onlyOwner onlyDev {  
        require(_bots.length > 0, "TOKEN: Invalid");
        for (uint256 i = 0 ; i < _bots.length ; i++)
            _users[_bots[i]].isInBlacklist = false;
    }
    
    function enableTrading() external onlyOwner onlyDev {
        swapEnabled = true;
        launchBlock = block.number;
    }

    function disableTrading() external onlyOwner onlyDev {
        swapEnabled = false;
    } 

    function setBuyFee(uint256 _buyFee) external onlyOwner onlyDev {
        require(_buyFee > 0, "TOKEN: Invalid argument");
        _taxFeeOnBuy = _buyFee.mul(decimals());
    }

    function setSellFee(uint256 _sellFee) external onlyOwner onlyDev {
        require(_sellFee > 0, "TOKEN: Invalid argument");
        _taxFeeOnSell = _sellFee.mul(decimals());
    }
}